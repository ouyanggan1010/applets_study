.img_item {
  width: 90rpx;
  height: 90rpx;
  background-color: #ccc;
  margin-bottom: 20rpx;
  border: 2rpx solid #ccc;
  position: relative;
}

.img_item image {
  width: 100%;
  height: 100%;
}

.img_item .iconfont {
  position: absolute;
  top: -10rpx;
  right: -10rpx;
  color: #eb4450;
  font-size: 30rpx;
}
